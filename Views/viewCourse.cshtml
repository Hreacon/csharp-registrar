@Html.Partial("header.cshtml")

<h1>Current Students</h1>
<ul>
  @foreach(var student in Model.GetStudents())
  {
    <li>@student.GetName()</li>
  }
</ul>

<h2>Students Not Enrolled</h2>
<p>
  Click to enroll
</p>

<ul>
  @foreach(var student in Model.GetStudentsNotInCourse())
  {
    <li><a href="/course/@Model.GetId()/add/@student.GetId()">@student.GetName()</a></li>
  }
</ul>
<a href="/" class="link">Go Home</a>
